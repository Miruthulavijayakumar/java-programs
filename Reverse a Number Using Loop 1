import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner inn = new Scanner(System.in);
         int n = inn.nextInt();
         if (n <= 0) {
            System.out.println("Invalid Input");
        } else {
            int reverse = 0;
            while (n > 0) {       
                reverse = reverse * 10 + n % 10;
                n = n / 10;  
            }
            System.out.println(reverse);
        }
    }
    
}
