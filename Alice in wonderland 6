import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner inn = new Scanner(System.in);
        int num = inn.nextInt();
        int digit1 = num / 10;
        int digit2 = num % 10;
        int sum = digit1 + digit2;
        System.out.println("Bird said:" + num);
        System.out.println("Alice must go in path-" + sum);
    }
}
