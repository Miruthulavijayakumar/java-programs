import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner inn = new Scanner(System.in);
        double num = inn.nextDouble();   
        int square = inn.nextInt();     
        int base = inn.nextInt();        
        int power = inn.nextInt();       
        System.out.println((int)Math.floor(num));
        System.out.println((int)Math.ceil(num));
        System.out.println((int)Math.sqrt(square));
        System.out.println((int)Math.pow(base, power));  
    }
}
